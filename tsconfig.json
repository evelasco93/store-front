{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "lib": ["es6", "dom"],
      "types": ["node"], 
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "noImplicitAny": false,
      "moduleResolution": "node",
      "resolveJsonModule": true
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules"]
  }
  